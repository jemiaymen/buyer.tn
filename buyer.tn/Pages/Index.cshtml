@page
@model IndexModel


@{
    ViewData["Title"] = "Dialog";
    await Html.RenderPartialAsync("Partial/_ListTemplate");


}





    <ej-button id="btnOpen" text="Click" click="onclick" />

    <ej-button id="register" text="Click" click="regclick" />

    <ej-button id="changepw" text="Click" click="changeclick" />

    <script>

        function onclick() {
            $("#loginForm").ejDialog("open");
        }

	    function regclick(){
	        $("#registerForm").ejDialog("open");
	    }
	    function changeclick(){
            $("#changepasswordForm").ejDialog("open");
        }
    </script>

@section Styles {
    @{await Html.RenderPartialAsync("Partial/_ListStyle");}
}

@section Scripts {
    @{await Html.RenderPartialAsync("Partial/_ListScript");}
}